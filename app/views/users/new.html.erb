<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :age %>
<%= f.number_field :age %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :password %>
<%= f.text_field :password %>
<%= f.label "What's your favorite Pokemon?" %>
<%= f.text_field :favorite_pokemon %>
<%= f.label "What's your favorite Pokemon type?" %>
<%= f.text_field :favorite_type %>
<%= f.label :bio %>
<%= f.text_field :bio %>
<%= f.submit "Create Account" %>
<% end %>


